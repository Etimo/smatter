{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "type": "commonjs",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --testTimeout=5000",
    "start": "node -p 3001 dist/index.js",
    "compile": "npx tsc",
    "server": "nodemon --watch src -e \".ts\" --exec \"npm run compile\"",
    "testdb": "docker run -e MONGO_INITDB_ROOT_USERNAME=fake -e MONGO_INITDB_ROOT_PASSWORD=fake -p 27024:27017 mongo",
    "database": "docker run -e MONGO_INITDB_ROOT_USERNAME=fake -e MONGO_INITDB_ROOT_PASSWORD=fake -p 27019:27017 mongo",
    "dev": "concurrently \"npm run server\" \"nodemon --watch dist dist/index.js\" \"npm run database\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mongoose": "^5.11.97",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "mongoose": "^8.1.2",
    "nodemon": "^3.0.3",
    "ts-compile": "^2.0.6",
    "ts-jest": "^29.1.2"
  },
  "devDependencies": {
    "@testcontainers/mongodb": "^10.7.1",
    "supertest": "^6.3.4",
    "typescript": "^5.3.3"
  }
}
