import { Schema, ObjectId model} from "mongoose"

export type IFollower = {
    followingId: ObjectId
    followerId: ObjectId
}

export const followerSchema = new Schema<IFollower>({
  followingId:  {type: Schema.Types.ObjectId, required: true},
  followerId: { type: Schema.Types.ObjectId, required: true }
})

const Follower = model<IFollower>("Follower",followerSchema)